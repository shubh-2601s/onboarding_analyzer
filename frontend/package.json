{
  "name": "onboarding-analyzer-frontend",
  "version": "2.0.0",
  "description": "ðŸš€ Advanced Enterprise Analytics Platform - AI-Powered Funnel Optimization",
  "main": "index.js",
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:production": "GENERATE_SOURCEMAP=false npm run build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js --mode=server --port=8888",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lighthouse": "lighthouse http://localhost:3000 --view",
    "serve": "serve -s build -l 3000",
    "docker:build": "docker build -t onboarding-analyzer-frontend .",
    "docker:run": "docker run -p 3000:80 onboarding-analyzer-frontend",
    "precommit": "lint-staged && npm run type-check",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deps:update": "npx npm-check-updates -u && npm install",
    "deps:check": "npx npm-check-updates"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.5",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "recharts": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "zustand": "^4.4.7",
    "react-query": "^3.39.3",
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.8",
    "react-intersection-observer": "^9.5.3",
    "react-helmet-async": "^1.3.0",
    "use-debounce": "^10.0.0",
    "react-error-boundary": "^4.0.11",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.4",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^8.0.3",
    "jest-junit": "^16.0.0",
    "lighthouse": "^11.3.0",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "serve": "^14.2.1",
    "typescript": "^5.3.2",
    "webpack-bundle-analyzer": "^4.10.1",
    "@storybook/addon-essentials": "^7.6.1",
    "@storybook/addon-interactions": "^7.6.1",
    "@storybook/addon-links": "^7.6.1",
    "@storybook/blocks": "^7.6.1",
    "@storybook/react": "^7.6.1",
    "@storybook/react-webpack5": "^7.6.1",
    "@storybook/testing-library": "^0.2.2",
    "storybook": "^7.6.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint-config-prettier"
    ],
    "plugins": [
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "no-console": "warn",
      "no-debugger": "error",
      "react-hooks/exhaustive-deps": "warn",
      "jsx-a11y/alt-text": "error",
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "internal",
            "parent",
            "sibling",
            "index"
          ],
          "newlines-between": "always"
        }
      ]
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm run test:ci"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "license": "MIT",
  "keywords": [
    "analytics",
    "funnel-analysis",
    "ai-powered",
    "react",
    "dashboard",
    "onboarding",
    "machine-learning",
    "real-time"
  ],
  "author": {
    "name": "Your Company",
    "email": "contact@yourcompany.com",
    "url": "https://yourcompany.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourcompany/onboarding-analyzer"
  },
  "bugs": {
    "url": "https://github.com/yourcompany/onboarding-analyzer/issues"
  },
  "workspaces": [
    "packages/*"
  ]
}
